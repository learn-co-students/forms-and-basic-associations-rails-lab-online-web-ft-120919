<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
   <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <datalist>
  <%Artist.all.each do |artist|%>
  <%=artist.name%>
  <%end%>
  </datalist>

  <%=f.label :genre_name%>
  <%=f.collection_select :genre_id, Genre.all, :id, :name %>

  <%=f.fields_for :notes do |note| %>
  <%=note.text_field :content %>
  <%end%>
  
  <%= f.submit %>
<% end %>


