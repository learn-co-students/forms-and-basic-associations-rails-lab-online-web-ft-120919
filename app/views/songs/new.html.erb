<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%=f.label :artist_name%>
  <%=f.text_field :artist_name%>
  <%= f.select(:genre_id, Genre.all.collect {|u| [u.name, u.id]}, :prompt => 'Select') %> <br>
  <%=f.fields_for :notes do |l|%>
      <%=l.label :content%>
      <%=l.text_field :content%>

  <% end %>
  <br />
  <%= f.submit %>
<% end %>


