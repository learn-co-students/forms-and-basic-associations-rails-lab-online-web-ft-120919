<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  
  <%= f.label :notes %><br>
  <input type="textbox" name="song_notes_attributes_0_content">
  
  <br />

  <%= f.submit %>
<% end %>