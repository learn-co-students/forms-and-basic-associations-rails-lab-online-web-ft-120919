<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  
  <%= f.fields_for :notes do |note| %>
  <%= note.label :content %>
  <%= note.text_field :content %><br>

  
  <br>
  <% end %>


  <%= f.submit %>
<% end %>